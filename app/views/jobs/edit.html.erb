<% @page_title="Edit Job" %>

<%= form_for(@job, :url => {:action => "update", :id => @job.id}) do |f| %>

  <%= render partial: "form", locals: {:job => @job, :f => f} %>

  <div class="row">
    <div class="col">
      <%= f.submit "Submit Job", class: "btn blue-grey white-text", type: "submit" %>
    </div>
  </div>

<% end %>


