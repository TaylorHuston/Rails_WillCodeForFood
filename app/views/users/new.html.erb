<% @page_title="New User" %>

<%= form_for(@user) do |f| %>

  <%= render partial: "form", locals: {:user => @user, :f => f} %>

  <div class="row">
    <%= recaptcha_tags %>
  </div>

  <div class="row">
    <div class="col">
      <%= f.submit "Register", class: "btn blue-grey white-text", type: "submit" %>
    </div>
  </div>

<% end %>
