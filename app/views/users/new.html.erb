<% @page_title="New User" %>
<%= render partial: "shared/navigation" %>

<main>
  <div id="container" class="container">

    <div class="row">
      <%= render partial: "shared/left_sidebar" %>

      <div id="rightSidebar" class="col l8 m7 s12 offset-m5 offset-l4">

        <%= render partial: "shared/warning" %>

        <%= form_for(@user) do |f| %>

          <%= render partial: "form", locals: {:user => @user, :f => f} %>

          <div class="row">
            <%= recaptcha_tags %>
          </div>

          <div class="row">
            <div class="col">
              <%= f.submit "Register", class: "btn blue-grey white-text", type: "submit" %>
            </div>
          </div>

        <% end %>

      </div>
    </div>
  </div>
</main>
